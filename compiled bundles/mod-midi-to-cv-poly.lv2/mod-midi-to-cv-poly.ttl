@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/midi-to-cv-poly>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "MIDI to CV Polyphonic";

doap:developer [
    foaf:name "Jarno Verheesen";
    foaf:homepage <>;
    foaf:mbox <mailto:j.verheesen@hotmail.nl>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jarno@moddevices.com>;
    ];

    lv2:minorVersion 2;
    lv2:microVersion 0;

rdfs:comment """
convert a midi message to polyphonic control voltages 1/12 volt per note
""";

lv2:port
[
    a lv2:InputPort, atom:AtomPort;
    atom:bufferType atom:Sequence;
    atom:supports midi:MidiEvent;
    lv2:designation lv2:control;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "Input";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 1;
    lv2:symbol "CV1";
    lv2:name "CV Output1";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 2;
    lv2:symbol "CV2";
    lv2:name "CV Output2";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 3;
    lv2:symbol "CV3";
    lv2:name "CV Output3";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 4;
    lv2:symbol "CV4";
    lv2:name "CV Output4";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 5;
    lv2:symbol "vel";
    lv2:name "Velocity";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 6;
    lv2:symbol "trig1";
    lv2:name "Trigger1";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 7;
    lv2:symbol "trig2";
    lv2:name "Trigger2";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 8;
    lv2:symbol "trig3";
    lv2:name "Trigger3";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 9;
    lv2:symbol "trig4";
    lv2:name "Trigger4";
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 10 ;
    lv2:symbol "Octave" ;
    lv2:name "Octave";
    lv2:default 0 ;
    lv2:minimum -3 ;
    lv2:maximum 3 ;
    lv2:portProperty lv2:integer;
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 11 ;
    lv2:symbol "Pitch" ;
    lv2:name "Pitch";
    lv2:default 0 ;
    lv2:minimum -12 ;
    lv2:maximum 12 ;
    lv2:portProperty lv2:integer;
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 12 ;
    lv2:symbol "Semitone" ;
    lv2:name "Semitone";
    lv2:default 0 ;
    lv2:minimum -12 ;
    lv2:maximum 12 ;
    lv2:portProperty lv2:integer;
].
