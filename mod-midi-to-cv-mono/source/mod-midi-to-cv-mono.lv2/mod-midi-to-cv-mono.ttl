@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://moddevices.com/plugins/mod-devel/midi-to-cv-mono>
a lv2:Plugin, lv2:UtilityPlugin;
        doap:name "MIDI to CV mono" ;
        rdfs:comment """
convert a midi message to monophonic control voltage 1/12 volt per note
""" ;
        lv2:minorVersion 3 ;
        lv2:microVersion 0 ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
          a lv2:InputPort ,
          atom:AtomPort ;
          atom:bufferType atom:Sequence ;
          atom:supports midi:MidiEvent ;
          lv2:index 0 ;
          lv2:symbol "in" ;
          lv2:name "In" ;
        ] , 
        [
          a lv2:OutputPort, lv2:CVPort;
          lv2:index 1;
          lv2:symbol "Pitch";
          lv2:name "Pitch";
        ] , 
        [
           a lv2:OutputPort, lv2:CVPort;
           lv2:index 2;
           lv2:symbol "Velocity";
           lv2:name "Velocity";
        ] , 
        [
           a lv2:OutputPort, lv2:CVPort;
           lv2:index 3;
           lv2:symbol "Gate";
           lv2:name "Gate";
        ] , 
        [
           a lv2:InputPort, lv2:ControlPort;
           lv2:index 4;
           lv2:symbol "Octave";
           lv2:name "Octave";
           lv2:minimum -3;
           lv2:default 0;
           lv2:maximum 3;
           lv2:portProperty lv2:integer;
        ] , 
        [
           a lv2:InputPort, lv2:ControlPort;
           lv2:index 5;
           lv2:symbol "Semitone";
           lv2:name "Semitone";
           lv2:minimum -12;
           lv2:default 0;
           lv2:maximum 12;
           lv2:portProperty lv2:integer;
        ] ,
        [
           a lv2:InputPort, lv2:ControlPort;
           lv2:index 6;
           lv2:symbol "Cent";
           lv2:name "Cent";
           lv2:minimum -100;
           lv2:default 0;
           lv2:maximum 100;
           lv2:portProperty lv2:integer;
        ] ,
        [
           a lv2:InputPort ,
           lv2:ControlPort ;
           lv2:index 7 ;
           lv2:symbol "Retrigger" ;
           lv2:name "Retrigger" ;
           lv2:default 0.0 ;
           lv2:minimum 0.0 ;
           lv2:maximum 1.0 ;
           lv2:portProperty lv2:toggled;
        ] ;

        doap:developer [
            foaf:name "Jarbi Verheesen" ;
            foaf:homepage <> ;
            foaf:mbox <mailto:j.verheesen@hotmail.nl> ;
        ] ;

        doap:maintainer [
            foaf:name "MOD Team" ;
            foaf:homepage <http://moddevices.com> ;
            foaf:mbox <mailto:devel@moddevices.com> ;
        ] ;

        mod:brand "MOD" ;
        mod:label "MIDI to CV mono" .
